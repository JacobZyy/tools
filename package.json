		"@types/node": "^18.8.4",
		"@typescript-eslint/eslint-plugin": "^5.40.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-jasmine": "^4.1.3",
		"eslint-plugin-prettier": "^4.2.1",
		"jasmine": "^4.4.0",
		"jest": "^29.1.2",
		"rimraf": "^3.0.2",
		"ts-jest": "^29.0.3",
		"tsup": "^6.2.3",
		"typescript": "^4.8.4",
		"unbuild": "^0.8.11",
		"unocss": "^0.45.29",
		"unplugin-icons": "^0.14.11"
	}
