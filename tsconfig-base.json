{
	"compilerOptions": {
		"rootDir": "./src",
		"outDir": "./lib",
		"target": "ES2019",
		"module": "CommonJS",
		"declaration": true,
		"declarationMap": false,
		"sourceMap": false,
		"composite": true,
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"importsNotUsedAsValues": "error"
	}
}
